  <concept>
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Includes drugs that have a high potential for abuse, no currently accepted medical use in the United States and a lack of accepted safety for use under medical supervision."/>
    </extension>
    <code value="I"/>
    <display value="Schedule I"/>
    <definition value="Schedule I"/>
  </concept>
  <concept>
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Includes drugs having currently accepted medical use in the United States and a high abuse potential, with severe psychological or physical dependence liability."/>
    </extension>
    <code value="II"/>
    <display value="Schedule II"/>
    <definition value="Schedule II"/>
  </concept>
  <concept>
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Includes drugs having an abuse potential less than that of drugs listed in Schedules I and II. All CS III drugs have a currently accepted medical use in the United States."/>
    </extension>
    <code value="III"/>
    <display value="Schedule III"/>
    <definition value="Schedule III"/>
  </concept>
  <concept>
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Includes drugs having a lesser potential for abuse than those listed in Schedule III. CS IV drugs have a currently accepted medical use in the United States."/>
    </extension>
    <code value="IV"/>
    <display value="Schedule IV"/>
    <definition value="Schedule IV"/>
  </concept>
  <concept>
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Includes drugs having low abuse potential and limited physical or psychological dependence relative to those listed in IV and have an accepted medical use in the United States."/>
    </extension>
    <code value="V"/>
    <display value="Schedule V"/>
    <definition value="Schedule V"/>
  </concept>
  <concept>
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="State defined"/>
    </extension>
    <code value="VI"/>
    <display value="Schedule VI"/>
    <definition value="Schedule VI"/>
  </concept>
