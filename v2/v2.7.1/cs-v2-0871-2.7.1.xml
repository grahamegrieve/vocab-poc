  <concept id="1">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Material is corrosive and may cause severe injury to skin, mucous membranes and eyes. Avoid any unprotected contact."/>
    </extension>
    <code value="COR"/>
    <display value="Corrosive"/>
    <definition value="Corrosive"/>
  </concept>
  <concept id="2">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Material is highly flammable and in certain mixtures (with air) may lead to explosions.  Keep away from fire, sparks and excessive heat."/>
    </extension>
    <code value="FLA"/>
    <display value="Flammable"/>
    <definition value="Flammable"/>
  </concept>
  <concept id="3">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Material is an explosive mixture.  Keep away from fire, sparks, and heat."/>
    </extension>
    <code value="EXP"/>
    <display value="Explosive"/>
    <definition value="Explosive"/>
  </concept>
  <concept id="4">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Material is solid and sharp (e.g., cannulas.)  Dispose in hard container."/>
    </extension>
    <code value="INJ"/>
    <display value="Injury Hazard"/>
    <definition value="Injury Hazard"/>
  </concept>
  <concept id="5">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Material is toxic to humans and/or animals.  Special care must be taken to avoid incorporation, even of small amounts."/>
    </extension>
    <code value="TOX"/>
    <display value="Toxic"/>
    <definition value="Toxic"/>
  </concept>
  <concept id="6">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Material is a source for ionizing radiation and must be handled with special care to avoid injury of those who handle it and to avoid environmental hazards."/>
    </extension>
    <code value="RAD"/>
    <display value="Radioactive"/>
    <definition value="Radioactive"/>
  </concept>
  <concept id="7">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Material hazard level is unknown."/>
    </extension>
    <code value="UNK"/>
    <display value="Unknown"/>
    <definition value="Unknown"/>
  </concept>
