      <value value="address line"/>
    </designation>
    <concept id="17887">
      <code value="DAL"/>
      <definition value=" A delivery address line is frequently used instead of breaking out delivery mode, delivery installation, etc.  An address generally has only a delivery address line or a street address line, but not both."/>
      <designation>
        <use>
          <system value="http://something...?"/>
          <code value="deprecated alias"/>
        </use>
        <value value="delivery address line"/>
      </designation>
    </concept>
    <concept id="14822">
      <code value="SAL"/>
